{
    "name": "gonzalo123/yaml2pimple",
    "keywords": ["pimple", "dependency injection", "container"],
    "description": "Build a Pimple/Container from a config file",
    "license":"MIT",
    "authors": [
        {
            "name": "Gonzalo Ayuso",
            "email": "gonzalo123@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "G\\Yaml2Pimple\\": "src/"
        }
    },
    "require": {
        "pimple/pimple": "~3.0.",
        "symfony/config": "~2.3",
        "symfony/yaml": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "phpunit/php-code-coverage": "~2.0",
        "phpunit/phpunit-mock-objects": "~2.3",
        "sebastian/global-state": "~1.0@dev"
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "branch-alias": {
            "dev-sf23": "1.0.x-dev"
        }
    }
}
